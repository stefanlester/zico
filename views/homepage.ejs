    <script>
      document.addEventListener('DOMContentLoaded', function () {
        // Function to fetch cryptocurrency data
        async function fetchCryptoData() {
          const apiUrl =
            'https://api.coingecko.com/api/v3/coins/markets?vs_currency=usd&ids=bitcoin,ethereum,binancecoin,tether,solana,ripple,cardano,avalanche';
          try {
            const response = await fetch(apiUrl);
            const data = await response.json();
            updateTable(data);
          } catch (error) {
            console.error('Error fetching data:', error);
          }
        }

        // Function to update the HTML table with fetched data
        function updateTable(data) {
          const tableBody = document.querySelector('.coin-list__main .table tbody');
          tableBody.innerHTML = ''; // Clear existing table rows

          data.forEach((crypto, index) => {
            const row = document.createElement('tr');
            row.innerHTML = `
                        <th scope="row"><span class="icon-star"></span></th>
                        <td>${index + 1}</td>
                        <td>
                            <a href="#">
                                <span>${crypto.name}</span>
                                <span class="unit">${crypto.symbol.toUpperCase()}</span>
                            </a>
                        </td>
                        <td class="boild">$${crypto.current_price.toLocaleString()}</td>
                        <td class="${crypto.price_change_percentage_24h > 0 ? 'up' : 'down'}">${crypto.price_change_percentage_24h.toFixed(2)}%</td>
                        <td class="boild">$${(crypto.market_cap / 1e9).toFixed(2)} B</td>
                        <td><div id="total-revenue-chart-${index}"></div></td>
                        <td><a href="#" class="btn">Trade</a></td>
                    `;
            tableBody.appendChild(row);
          });
        }

        // Call the fetch function to update the table with live data
        fetchCryptoData();
      });
    </script>